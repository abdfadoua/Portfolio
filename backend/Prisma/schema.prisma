generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model message {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  subject   String
  message   String
  createdAt DateTime @default(now())
  @@map("Messages")
}
model admin {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  @@map("admins")
}